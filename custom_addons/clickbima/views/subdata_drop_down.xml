<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="subdatadropdown">
      <field name="name">subdata.subdata list</field>
      <field name="model">subdata.subdata</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="subdatadropdownform">
      <field name="name">subdata.subdata form</field>
      <field name="model">subdata.subdata</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="subdatadropdownactionwindow">
      <field name="name">Info Sub Data(Admin)</field>
      <field name="res_model">subdata.subdata</field>
      <field name="view_mode">tree,form,graph,pivot,kanban</field>
    </record>

