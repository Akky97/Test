<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="detailregistrationlist">
      <field name="name">detailregistration list</field>
      <field name="model">detailregistration</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
         
        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="detailregistrationform">
      <field name="name">detailregistration form</field>
      <field name="model">detailregistration</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>


            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="detailregistration_action_window">
      <field name="name">Detail Registration</field>
      <field name="res_model">detailregistration</field>
      <field name="view_mode">tree,form,graph,pivot,kanban</field>
    </record>

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="detailregistration_action_server">
      <field name="name">detailregistration server</field>
      <field name="model_id" ref="model_detailregistration"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

    <menuitem name="Detail Registration" id="list_121" parent="clickbima.menu_8" action="detailregistration_action_window" sequence="1"/>

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>