<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="mappingposlist">
      <field name="name">mappingpos list</field>
      <field name="model">mappingpos</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>

        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="mappingposform">
      <field name="name">mappingpos form</field>
      <field name="model">mappingpos</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>

            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="mappingpos_action_window">
      <field name="name">Mapping POS</field>
      <field name="res_model">mappingpos</field>
      <field name="view_mode">tree,form,graph,pivot,kanban</field>
    </record>

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="mappingpos_action_server">
      <field name="name">mappingpos server</field>
      <field name="model_id" ref="model_mappingpos"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

    <menuitem name="Mapping POS" id="list_12" parent="clickbima.menu_2" action="mappingpos_action_window" sequence="12"/>

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>