<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="companylist">
      <field name="name">company list</field>
      <field name="model">company</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>

        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="companyform">
      <field name="name">company form</field>
      <field name="model">company</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>

            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="company_action_window">
      <field name="name">company window</field>
      <field name="res_model">company</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="company_action_server">
      <field name="name">company server</field>
      <field name="model_id" ref="model_company"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

    <menuitem name="Insurer Branch" id="list_2" parent="clickbima.menu_1" action="company_action_window" sequence="2"/>

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>