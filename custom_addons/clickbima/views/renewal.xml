<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="renewalcalllist">
      <field name="name">renewalcall list</field>
      <field name="model">renewalcall</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
         
        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="renewalcallform">
      <field name="name">renewalcall form</field>
      <field name="model">renewalcall</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>


            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="renewalcall_action_window">
      <field name="name">Renewal Calling</field>
      <field name="res_model">renewalcall</field>
      <field name="view_mode">tree,form,graph,pivot,kanban</field>
    </record>

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="renewalcall_action_server">
      <field name="name">renewalcall server</field>
      <field name="model_id" ref="model_renewalcall"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

    <menuitem name="Renewal Calling" id="list_123" parent="clickbima.menu_8" action="renewalcall_action_window" sequence="3"/>

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>