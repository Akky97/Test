<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="uploaddatalist">
      <field name="name">uploaddata list</field>
      <field name="model">uploaddata</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
         
        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="uploaddataform">
      <field name="name">uploaddata form</field>
      <field name="model">uploaddata</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>


            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="uploaddata_action_window">
      <field name="name">Upload Data</field>
      <field name="res_model">uploaddata</field>
      <field name="view_mode">tree,form,graph,pivot,kanban</field>
    </record>

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="uploaddata_action_server">
      <field name="name">uploaddata server</field>
      <field name="model_id" ref="model_uploaddata"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

    <menuitem name="Upload Data" id="list_122" parent="clickbima.menu_8" action="uploaddata_action_window" sequence="2"/>

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>