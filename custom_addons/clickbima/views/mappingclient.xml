<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="mappingclientlist">
      <field name="name">mappingclient list</field>
      <field name="model">mappingclient</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>

        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="mappingclientform">
      <field name="name">mappingclient form</field>
      <field name="model">mappingclient</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>

            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="mappingclient_action_window">
      <field name="name">Mapping Client</field>
      <field name="res_model">mappingclient</field>
      <field name="view_mode">tree,form,graph,pivot,kanban</field>
    </record>

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="mappingclient_action_server">
      <field name="name">mappingclient server</field>
      <field name="model_id" ref="model_mappingclient"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

    <menuitem name="Mapping Client" id="list_13" parent="clickbima.menu_2" action="mappingclient_action_window" sequence="13"/>

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>