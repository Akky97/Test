<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="gsttaxlist">
      <field name="name">gsttax list</field>
      <field name="model">gsttax</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
            <field name="tax"/>

        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="gsttaxform">
      <field name="name">gsttax form</field>
      <field name="model">gsttax</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
              <group>
                  <field name="name"/>
                  <field name="tax"/>
              </group>


          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="gsttax_action_window">
      <field name="name">Taxes</field>
      <field name="res_model">gsttax</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="gsttax_action_server">
      <field name="name">gsttax server</field>
      <field name="model_id" ref="model_gsttax"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

    <menuitem name="Taxes" id="list_221231" parent="clickbima.menu_7" action="gsttax_action_window" sequence="3"/>

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>