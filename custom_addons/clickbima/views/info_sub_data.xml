<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="infosubdatalist">
      <field name="name">infosubdata list</field>
      <field name="model">infosubdata</field>
      <field name="arch" type="xml">
        <tree import="false" default_order="name asc">
          <field name="name"/>
<!--          <field name="bind"/>-->
          <field name="infoname"/>
          <field name="infosub1"/>

        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="infosubdataform">
      <field name="name">infosubdata form</field>
      <field name="model">infosubdata</field>
      <field name="arch" type="xml">
        <form  duplicate="0">
          <sheet>
            <group>
              <field name="name"  options="{'no_create': True, 'no_create_edit':True,'no_open':True}" style=""/>
<!--              <field name="bind" placeholder="Bind With"/>-->
              <field name="infoname" options="{'no_create': True, 'no_create_edit':True,'no_open':True}" style="" placeholder="Info Sub Name"/>
              <field name="infosub1" options="{'no_create': True, 'no_create_edit':True,'no_open':True}" style="" placeholder="Info Sub Data Name"/>

            </group>
          </sheet>
        </form>
      </field>
    </record>


    <record model="ir.ui.view" id="infosubdataform123456">
      <field name="name">Infosubdata search</field>
      <field name="model">infosubdata</field>
      <field name="arch" type="xml">
        <search>

              <field name="name"/>
              <field name="infoname" placeholder="Info Sub Name"/>
              <field name="infosub1" placeholder="Info Sub Data Name"/>
              <group expand="0" string="Group By">
                  <filter string="Info-Type Name" name="groupby_infotypename" context="{'group_by': 'name'}"/>
                  <filter string="Info Sub Name" name="groupby_infosubname" context="{'group_by': 'infoname'}"/>
                  <filter string="Info Sub Data Name" name="groupby_infosubdataname" context="{'group_by': 'infosub1'}"/>
              </group>
        </search>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="infosubdata_action_window">
      <field name="name">Info Sub Data(SA)</field>
      <field name="res_model">infosubdata</field>
      <field name="view_mode">tree,form,search,pivot,graph</field>
    </record>


    <!--<record model="ir.actions.act_window" id="infosubdata_action_window1">-->
      <!--<field name="name">Info Sub Data(Admin)</field>-->
      <!--<field name="res_model">infosubdata</field>-->
      <!--<field name="view_mode">tree,form,search,pivot,graph</field>-->
    <!--</record>-->

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="infosubdata_action_server">
      <field name="name">infosubdata server</field>
      <field name="model_id" ref="model_infosubdata"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

    <menuitem name="Info Sub Data Mapping(SA)" id="list_5"  parent="clickbima.menu_0" action="infosubdata_action_window" sequence="4"/>
    <!--<menuitem name="Info Sub Data(Admin)" id="list_53112"  parent="clickbima.menu_1" action="infosubdata_action_window1" sequence="-1"/>-->

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>