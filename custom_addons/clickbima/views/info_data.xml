<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="infodatalist">
      <field name="name">Info-Data Master(SA)</field>
      <field name="model">infodata</field>
      <field name="arch" type="xml">
        <tree import="false" default_order="name asc">
<!--          <field name="name"/>-->
          <field name="name" string="Info-Type Name" />
          <!--<field name="infodata1"/>-->
            <field name="infosubdata" string="Info-Type-Data Name"/>
            <field name="company_id" invisible="1"/>

        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="infodataform">
      <field name="name">Info-Data Master(SA)</field>
      <field name="model">infodata</field>
      <field name="arch" type="xml">
        <form duplicate="0">
          <sheet>
            <group>
<!--              <field name="name" placeholder="Info Type Code"/>-->
              <field name="name" style="" placeholder="Info Type" string="Info-Type Name" options="{'no_create': True, 'no_create_edit':True,'no_open':True}"/>
              <!--<field name="x_sub_data"/>-->
                <field name="infosubdata"  style="" placeholder="Info Data Name" string="Info-Type-Data Name"/>
                <!--<field name="infodata1" placeholder="Info Data Name"/>-->
                <field name="company_id" invisible="1"/>
            </group>
          </sheet>
        </form>

      </field>
    </record>



        <record model="ir.ui.view" id="infodatasearch">
      <field name="name">Info-Data Master(SA)</field>
      <field name="model">infodata</field>
      <field name="arch" type="xml">
           <search string="Search Info-Type Name">
            <field name="name"/>
            <field name="infosubdata"/>
               <group expand="0" string="Group By">
                  <filter string="Info-Type Name" name="groupby_infotypename" context="{'group_by': 'name'}"/>
                  <filter string="Info Type Data Name" name="groupby_infosubname" context="{'group_by': 'infosubname'}"/>
              </group>
         </search>
      </field>
    </record>



    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="infodata_action_window">
      <field name="name">Info-Data Master(SA)</field>
      <field name="res_model">infodata</field>
      <field name="view_mode">tree,form,search,pivot,graph</field>
    </record>


       <!--<record model="ir.actions.act_window" id="infodata_action_window1">-->
      <!--<field name="name">Info-Data Master(Admin)</field>-->
      <!--<field name="res_model">infodata</field>-->
      <!--<field name="view_mode">tree,form,search,pivot,graph</field>-->
    <!--</record>-->

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="infodata_action_server">
      <field name="name">Info-Data Master(SA)</field>
      <field name="model_id" ref="model_infodata"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>
      

<!--    <menuitem name="Info-Data Master(SA)" id="list_4" parent="clickbima.menu_0" action="infodata_action_window" sequence="2"/>-->
    <!--<menuitem name="Info-Data Master(Admin)" id="list_4123" parent="clickbima.menu_1" action="infodata_action_window1" sequence="-2"/>-->

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>