<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="infosubdatanamelist">
      <field name="name">infosubdataname list</field>
      <field name="model">infosubdatadropdown</field>
      <field name="arch" type="xml">
        <tree import="false" default_order="name asc">
          <field name="info_type" string="Info Type Name" options="{'no_create': True, 'no_create_edit':True,'no_open':True}"/>
           <field name="info_sub_name" placeholder="Info Sub Name" string="Info Sub Name" options="{'no_create': True, 'no_create_edit':True,'no_open':True}"/>
              <field name="name" placeholder="Info Sub Data Name" string="Info Sub Data Name"/>


        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="infosubdatanameform">
      <field name="name">infosubdataname form</field>
      <field name="model">infosubdatadropdown</field>
      <field name="arch" type="xml">
        <form  duplicate="0">
          <sheet>
            <group>
              <field name="info_type" style="" string="Info Type Name" options="{'no_create': True, 'no_create_edit':True,'no_open':True}"/>
<!--              <field name="bind" placeholder="Bind With"/>-->
              <field name="info_sub_name"  style="" placeholder="Info Sub Name" string="Info Sub Name" options="{'no_create': True, 'no_create_edit':True,'no_open':True}"/>
              <field name="name" placeholder="Info Sub Data Name" string="Info Sub Data Name"/>

            </group>
          </sheet>
        </form>
      </field>
    </record>


    <record model="ir.ui.view" id="infosubdatanamesearch">
      <field name="name">Infosubdataname search</field>
      <field name="model">infosubdatadropdown</field>
      <field name="arch" type="xml">
        <search>

              <field name="info_type" string="Info Type Name"/>
              <field name="info_sub_name" placeholder="Info Sub Name"/>
              <field name="name" placeholder="Info Sub Data Name"/>
              <group expand="0" string="Group By">
                  <filter string="Info-Type Name" name="groupby_infotypename" context="{'group_by': 'info_type'}"/>
                  <filter string="Info Sub Name" name="groupby_infosubname" context="{'group_by': 'info_sub_name'}"/>
<!--                  <filter string="Info Sub Data Name" name="groupby_infosubdataname" context="{'group_by': 'infosub1'}"/>-->
              </group>
        </search>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="infosubdataname_action_windows">
      <field name="name">Info Sub Data Name(SA)</field>
      <field name="res_model">infosubdatadropdown</field>
      <field name="view_mode">tree,form,search,pivot,graph</field>
    </record>


<!--    <record model="ir.actions.act_window" id="infosubdata_action_window1">-->
<!--      <field name="name">Info Sub Data(Admin)</field>-->
<!--      <field name="res_model">infosubdata</field>-->
<!--      <field name="view_mode">tree,form,search,pivot,graph</field>-->
<!--    </record>-->

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="infosubdataname_action_server">
      <field name="name">infosubdataname server</field>
      <field name="model_id" ref="model_infosubdatadropdown"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

