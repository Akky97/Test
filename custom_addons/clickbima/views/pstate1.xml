<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="pstate1list">
      <field name="name">pstate1 list</field>
      <field name="model">pstate1</field>
      <field name="arch" type="xml">
        <tree>
          <field name="dockno"/>


        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="pstate1form">
      <field name="name">pstate1 form</field>
      <field name="model">pstate1</field>
      <field name="arch" type="xml">
        <form>
            <div style="margin: -7px;"><h2>Policy Status</h2></div>
             <group>
    <div class="row ist_table">
        <div class="col-md-12">
            <table>
        <tr>
            <th style="width: 20%; text-align: center; border: 1px solid #000; color: #7c7bad; background: #eae8ea;">Docket No</th>
            <th style="width: 10%; text-align: center; border: 1px solid #000; color: #7c7bad; background: #eae8ea;">Serial No</th>
            <th style="width: 10%; text-align: center; border: 1px solid #000; color: #7c7bad; background: #eae8ea;">Policy No</th>
            <th style="width: 10%; text-align: center; border: 1px solid #000; color: #7c7bad; background: #eae8ea;">Dispatch Date</th>
            <th style="width: 10%; text-align: center; border: 1px solid #000; color: #7c7bad; background: #eae8ea;">Dispatch Detail</th>
            <th style="width: 10%; text-align: center; border: 1px solid #000; color: #7c7bad; background: #eae8ea;">Courier Detail</th>
            <th style="width: 10%; text-align: center; border: 1px solid #000; color: #7c7bad; background: #eae8ea;">Status Type</th>
            <th style="width: 10%; text-align: center; border: 1px solid #000; color: #7c7bad; background: #eae8ea;">Policy Recd. Type</th>
            <th style="width: 10%; text-align: center; border: 1px solid #000; color: #7c7bad; background: #eae8ea;">Policy Send Type</th>
        </tr>
                <tr>
                    <td><field name="dockno"/></td>
                    <td><field name="serialno"/></td>
                    <td><field name="policyno12"/></td>
                    <td><field name="disdate"/></td>
                    <td><field name="disdetail"/></td>
                    <td><field name="courierdet"/></td>
                    <td><field name="statustype"/></td>
                    <td><field name="recdtype"/></td>
                    <td><field name="sendtype"/></td>
                </tr>
    </table>
        </div>
    </div>
          </group>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="pstate1_action_window">
      <field name="name">Policy status</field>
      <field name="res_model">pstate1</field>
      <field name="view_mode">tree,form,graph,pivot,kanban</field>
    </record>

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="pstate1_action_server">
      <field name="name">pstate1 server</field>
      <field name="model_id" ref="model_pstate1"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

<menuitem name="Policy Status" id="list_1232345" parent="clickbima.menu_9" action="pstate1_action_window" sequence="1"/>

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>