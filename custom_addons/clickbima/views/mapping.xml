<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="mappinglist">
      <field name="name">mapping list</field>
      <field name="model">mapping</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>

        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="mappingform">
      <field name="name">mapping form</field>
      <field name="model">mapping</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>

            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="mapping_action_window">
      <field name="name">Mapping SM/RM</field>
      <field name="res_model">mapping</field>
      <field name="view_mode">tree,form,graph,pivot,kanban</field>
    </record>

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="mapping_action_server">
      <field name="name">mapping server</field>
      <field name="model_id" ref="model_mapping"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

    <menuitem name="Mapping SM/RM" id="list_11" parent="clickbima.menu_2" action="mapping_action_window" sequence="11"/>

<!--    <menuitem id="Airspring optionalportnrc" name="AirSpring Master Optional Port NRC" parent="air_spring1.menu_2" action="optionalportnrc" sequence="8"/>-->
  </data>
</odoo>