<odoo>
  <data>
    <!-- explicit list view definition -->
    <record model="ir.ui.view" id="headermarklist">
      <field name="name">headermark list</field>
      <field name="model">headermark</field>
      <field name="arch" type="xml">
        <tree duplicate="0">
          <field name="name"/>
          <field name="remark"/>
        </tree>
      </field>
    </record>

      <record model="ir.ui.view" id="headermarkform">
      <field name="name">headermark form</field>
      <field name="model">headermark</field>
      <field name="arch" type="xml">
        <form duplicate="0">
          <sheet>
            <group>
                <field name="name" />
                <field name="remark" />
            </group>
          </sheet>
        </form>
      </field>
    </record>

    <!-- actions opening views on models -->
    <record model="ir.actions.act_window" id="headermark_action_window">
      <field name="name">header Rate</field>
      <field name="res_model">headermark</field>
      <field name="view_mode">tree,form,graph,pivot,kanban</field>
    </record>

    <!-- server action to the one above -->
    <record model="ir.actions.server" id="headermark_action_server">
      <field name="name">headermark server</field>
      <field name="model_id" ref="model_headermark"/>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": self._name,
        }
      </field>
    </record>

