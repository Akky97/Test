<odoo>
    <data>
        <record id="course_liked_report_form_view" model="ir.ui.view">
            <field name="name">course.liked.report.form</field>
            <field name="model">course.liked.report</field>
            <field name="arch" type="xml">
                <form string="Course Liked Report">
                <group>
                	<group >
                		<field name="course_journey" required="1" widget="radio"/>
                        <!-- <field name="user_ids" widget="many2many_tags"/> -->
                        <!-- <field name="region" required="1"/> -->
                    </group>
                </group>
                     <footer>
                         <button name="button_print" string="Print" type="object" class="oe_highlight" />
                         <button string="Cancel" class="oe_link" special="cancel" /> 
                    </footer>
                </form>
            </field>
        </record>
        <record id="action_course_liked_report_print" model="ir.actions.act_window">
            <field name="name">Course Liked Report</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">course.liked.report</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        
        
       <record id="course_liked_view_report_id" model="ir.ui.view">
			<field name="name">Course Liked Report View</field>
			<field name="model">course.liked.view.report</field>
			<field name="arch" type="xml">
				<form string="Excel Report file" create="false" edit="false" duplicate="false" delete="false">
                    <group>
                        <field name="excel_file" readonly="1" filename="file_name" />
                        <field name="file_name" invisible="1" />
				    </group>
				</form>
			</field>
		</record> 
		
		<menuitem id="action_course_liked_report_print_menu" name="Course Liked Report" parent="website_slides.website_slides_menu_report" action="action_course_liked_report_print" sequence="4"/>
    </data>
</odoo>