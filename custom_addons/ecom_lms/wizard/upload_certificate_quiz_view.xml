<odoo>
    <data>
        <record id="upload_certificate_quiz_form_view" model="ir.ui.view">
            <field name="name">upload.certificate.quiz.form</field>
            <field name="model">upload.certificate.quiz</field>
            <field name="arch" type="xml">
                <form string="Upload Certificate Quiz">
               		<notebook>
                            <page string="Quiz" >
                                <field name="quiz_ids" >
                                    <tree editable="bottom">
                                        <field name="sequence" widget="handle"/>
						                <field name="title"/>
						                <field name="survey_id"/>
						                <field name="question_type"/>
						                <field name="triggering_question_id" invisible="1"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>
                     <footer>
                         <!-- <button name="button_print" string="Print" type="object" class="oe_highlight" /> -->
                         <button string="Cancel" class="oe_link" special="cancel" /> 
                    </footer>
                </form>
            </field>
        </record>
        <record id="action_upload_certificate_quiz_view" model="ir.actions.act_window">
            <field name="name">Upload Certificate Quiz</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">upload.certificate.quiz</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
        
    </data>
</odoo>