      <t t-name="website.desktop">
  <t t-call="website.layout">
      <style>
               .accordion .card-header:after {
                font-family: 'FontAwesome';
                content: "\f056";
                float: right;
            }
            .accordion .card-header.collapsed:after {
                /* symbol for "collapsed" panels */
                content: "\f055";
            }
            .card-img-top{
            padding-top: 50%;
            background-image: url(/web/image/slide.channel/1/image_1024?unique=83d4aab);
            background-size: cover;
            background-position: center;
          }
          .o_wslides_desc_truncate_3{
            white-space: nowrap;
            width: 251px;
            overflow: hidden;
            text-overflow: ellipsis;
            height: 27px;
          }
           .view_all_card_text_section_btn {
                    display:none;
                    padding: 4px 5px;
                    font-size: 7px;
                }
          @media(max-width: 765px) {
                        .view_all_card_text_section_btn{
                             display:block;
                            padding: 4px 5px;
                            font-size: 12px;
                        }
                      .pwa_filter_page_sub_para{
                      font-size:11px
                      }
                      .pwa_filter_view_card_head{
                            white-space: nowrap;
                            width: 128px;
                            overflow: hidden;
                            text-overflow: ellipsis;
                            height: 27px;
                      }
                      .o_wslides_desc_truncate_3{
                        white-space: nowrap;
                        width: 138px;
                        overflow: hidden;
                        text-overflow: ellipsis;
                        height: 27px;
                          }
                      .container, .o_container_small {
                        max-width: 586px;
                        }
                        .pwa_filter_section{
                         display:none;
                        }
              .filter_short_by_section{
              display:none;
              }

              .filter_name_heading_dots {
                    margin: -5px 16px;
                }
              .card-img-top{
                padding-top: 75% !important;
                background-image: url(/web/image/slide.channel/1/image_1024?unique=83d4aab);
                background-size: cover;
                background-position: center;
              }
          }
              @media(max-width: 330px) {
                .page-link {
                    position: relative;
                    display: block;
                    /* padding: 0.5rem 0.75rem; */
                    margin-left: -1px;
                    line-height: 1.25;
                    color: #267372;
                    background-color: #FFFFFF;
                    border: 1px solid #DEE2E6;
                    padding: 8px 6px;
                }
                  .pagination {
                        display: -webkit-box;
                        display: -webkit-flex;
                        display: flex;
                        padding-left: 4px;
                        list-style: none;
                        border-radius: 0.25rem;
                    }
                    .card-img-top {
                        padding-top: 114% !important;
                        background-image: url(/web/image/slide.channel/1/image_1024?unique=83d4aab);
                        background-size: cover;
                        background-position: center;
                        width: 132%;
                        float: right;
                    }
                .o_wslides_desc_truncate_3 {
                    white-space: nowrap;
                    width: 138px;
                    overflow: hidden;
                    text-overflow: ellipsis;
                    height: 21px;
                    font-size: 12px;
                }
                .filter_name_heading_dots {
                    margin: -11px 4px;
                }
                  .pwa_filter_view_card_head{
                    font-size: 20px;
                    }
                  .pwa_filter_page_sub_para{
                  font-size: 10px;
                  }
                .view_all_card_text_section_btn {
                    padding: 4px 5px;
                    font-size: 7px;
                }
              }



      </style>
