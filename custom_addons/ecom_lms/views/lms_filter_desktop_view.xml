              <div class="filter_short_by_section">
                <i class="fa fa-1x rounded fa-sort-amount-asc"/>
                <span>Sort by</span>
                <select>
                  <option value="value">Best Match</option>
                </select>
              </div>
            </div>
            <section class="s_text_image o_colored_level pt10 pb10" data-snippet="s_image_text" data-name="Image - Text" style="">
              <div class="container">

                  <div t-if="not channels and not search_term and not search_slide_type and not search_my and not search_tags and not search_channel_tag_id">
                        <p class="h2">No course available yet.</p>
<!--                        <p groups="website_slides.group_website_slides_officer">Click on "New" in the top-right corner to write your first course.</p>-->
                    </div>
                    <div t-elif="search_term and not channels" class="alert alert-info mb-5">
                        No course was found matching your search <code><t t-esc="search_term"/></code>.
                    </div>
                    <div t-elif="not channels" class="alert alert-info mb-5">
                        No course was found matching your search.
                    </div>
                  <div t-else="">
                       <t t-foreach="channels" t-as="channel">

                <div class="row pt-3 shadow border rounded p-3 mb-2">

                  <div class=" o_colored_level bg-white col-lg-4 col-4">
                            <t t-set="course_image_val" t-value="website.image_url(channel, 'image_1024')"/>
                            <div t-if="channel.image_1024" class="card-img-top" t-attf-style=" background-image: url(#{course_image_val});"/>

                            <div t-else="" class="o_wslides_gradient card-img-top position-relative" style="padding-top: 50%; opacity: 0.8">
                            <i class="fa fa-graduation-cap fa-2x mr-3 mb-3 position-absolute text-white-75" style="right:0; bottom: 0"/>
                            </div>
                  </div>
                  <div class=" o_color o_colored_level bg-white col-lg-8 col-8">
                    <div class="d-flex justify-content-between">
                      <span class="filter_name_heading">
                        <h3 class="pwa_filter_view_card_head" ><a class="card-title h5 mb-2 o_wslides_desc_truncate_2" t-attf-href="/pwa/slides/#{slug(channel)}" t-esc="channel.name"/></h3>
                      </span>
                      <div class="filter_name_heading_dots">
                        <i class="fa fa-1x mt-3 fa-ellipsis-v"/>
                      </div>
                    </div>
                    <div class="font-weight-light o_wslides_desc_truncate_3" t-field="channel.description_short"/>
                <div class="d-flex justify-content-between">
                      <div>
