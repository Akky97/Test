<?xml version="1.0" ?>
<odoo>
 <data noupdate="1">
    <record id="course_completed_schedular" model="ir.cron">
        <field name="name">Course Completed Delay Schedular</field>
        <field name="model_id" ref="model_slide_channel_partner"/>
        <field name="state">code</field>
        <field name="code">model.course_delay_method()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="True"/>
    </record>
    
    <record id="course_in_progress_delayed_schedular" model="ir.cron">
        <field name="name">Course In Progress Delay Schedular</field>
        <field name="model_id" ref="model_res_users"/>
        <field name="state">code</field>
        <field name="code">model.course_delay_inprogress_method()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="True"/>
    </record>
    
    <record id="employee_user_create_schedular" model="ir.cron">
        <field name="name">Employee User Schedular</field>
        <field name="model_id" ref="model_res_users"/>
        <field name="state">code</field>
        <field name="code">model.emp_user_create()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="True"/>
    </record>

      <record id="course_startlive_start_schedular" model="ir.cron">
        <field name="name">Live Session Start</field>
        <field name="model_id" ref="model_slide_channel"/>
        <field name="state">code</field>
        <field name="code">model.newpublish_url()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="True"/>
          <field name="nextcall" eval="(DateTime.now().strftime('%Y-%m-%d 07:00:00'))" />
    </record>
 </data>
</odoo>